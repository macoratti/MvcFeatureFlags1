{
  "ConnectionStrings": {
    "Sqlite": "Data Source=CatalogDB.db"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "FeatureManagement": {
    "CreateProduct": false,
    "EditProduct": true,
    "DeleteProduct": false,
    "ProductList": false,
    "SomeThirdPartyFeature": true,
    "ExportProduct": {
      "EnabledFor": [
        {
          "Name": "Percentage",
          "Parameters": {
            "Value": 50
          }
        }
      ]
    },
    "ProductSaleOff": {
      "EnabledFor": [
        {
          "Name": "Microsoft.TimeWindow",
          "Parameters": {
            "Start": "15 Jan 2024 02:00:00 +00:00",
            "End": "20 Jan 2024 01:00:00 +00:00"
          }
        }
      ]
    }
  }
}